[comment: we will show all of this user's created itineraries below]
<h1>Itineraries Created</h1>
  <table>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <%= current_user.name %>
    </tr>
<% @itineraries.try(:each) do |itinerary| %>
    <tr>
      <td><%= itinerary.id %></td>
      <td><%= link_to itinerary.name, itinerary_path(itinerary) %></td>
    </tr>
<% end %>
  </table>

